<h1>Welcome. Sign In Below to View ChiChi's Secret Recipe Book</h1>
<%= form_for @user, url: sessions_path do |f| %>
  <%= f.label "Name:" %>
  <%= f.text_field :name %>
  <br></br>
  <%= f.label "Email:" %>
  <%= f.text_field :email %>
  <br></br>
  <%= f.label "Password:" %>
  <%= f.password_field :password %>
  <br></br>
  <%= f.submit "Sign Up" %>
<% end %>
